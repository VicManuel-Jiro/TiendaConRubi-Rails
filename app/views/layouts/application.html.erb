<!DOCTYPE html>
<html>
  <head>
    <title>Lpoo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag    'bootstrap', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'bootstrap', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="/path/to/bootstrap-hover-dropdown.min.js"></script>
  </head>

  <body onload="obtenerTiempo()">
    <%= render 'layouts/header'%>
    <div class="formulario">
    <%= yield %>
    </div>
    <%= render 'layouts/footer'%>
  </body>
</html>
